function get_data(labelPercentage)
% get_data  Randomly determines labeled Indices 
%   C = ADDME(A) adds A to itself.
%
%   C = ADDME(A,B) adds A and B together.
%
%   See also SUM, PLUS.

% labeled Indices
labeledIndices = zeros(labelPercentage, 10);
%%for new computation of random seeds:
for i = 1:10
    perm = randperm(anz(i));
    seeds(((i-1)*labelPercentage+1):(i*labelPercentage)) = L(i,perm(1:labelPercentage));
end



end